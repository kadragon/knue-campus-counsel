{
  "name": "knue-campus-counsel",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "check": "npm run typecheck && npm run test",
    "webhook:set": "node scripts/webhook.mjs set",
    "webhook:delete": "node scripts/webhook.mjs delete",
    "webhook:info": "node scripts/webhook.mjs info"
  },
  "devDependencies": {
    "typescript": "^5.5.4",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
